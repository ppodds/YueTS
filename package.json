{
  "name": "yuets",
  "private": "true",
  "description": "A TypeScript rewrite version of Discord bot Yue.",
  "type": "commonjs",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "start": "node --no-deprecation --no-warnings ./dist/index.js",
    "dev": "concurrently 'npm:watch' 'sleep 5;nodemon ./dist/bot.js'",
    "test": "jest",
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppodds/YueTS.git"
  },
  "author": "ppodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppodds/YueTS/issues"
  },
  "lint-staged": {
    "src/**/*.{ts,js,cjs,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/ppodds/YueTS#readme",
  "dependencies": {
    "@discordjs/builders": "~1.10.1",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.4.3",
    "@discordjs/voice": "^0.18.0",
    "@discordx/importer": "^1.3.1",
    "@ppodds/nhentai-api": "^0.0.1",
    "async-lock": "^1.4.1",
    "axios": "^1.7.2",
    "bufferutil": "^4.0.8",
    "canvas": "^3.1.0",
    "discord-api-types": "^0.37.119",
    "discord.js": "^14.18.0",
    "discord.js-message-listener": "^3.0.2",
    "discordx": "^11.12.2",
    "dotenv": "^16.4.5",
    "ehentai-api": "^0.0.4",
    "file-type": "^16.5.4",
    "log4js": "^6.9.1",
    "mariadb": "^3.3.1",
    "node-worker-threads-pool": "^1.5.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sharp": "~0.33.4",
    "sodium-native": "^4.3.3",
    "stream-buffers": "^3.0.3",
    "tslib": "^2.6.3",
    "tsyringe": "^4.8.0",
    "utf-8-validate": "^6.0.4",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.11",
    "@types/sequelize": "^4.28.20",
    "@types/stream-buffers": "^3.0.7",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "version": "1.1.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@discordjs/opus",
      "canvas"
    ]
  }
}
