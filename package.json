{
  "name": "yuets",
  "private": "true",
  "description": "A TypeScript rewrite version of Discord bot Yue.",
  "type": "commonjs",
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "start": "node --no-deprecation --no-warnings ./dist/index.js",
    "dev": "concurrently 'npm:watch' 'sleep 5;nodemon ./dist/bot.js'",
    "test": "jest",
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppodds/YueTS.git"
  },
  "author": "ppodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppodds/YueTS/issues"
  },
  "lint-staged": {
    "src/**/*.{ts,js,cjs,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/ppodds/YueTS#readme",
  "dependencies": {
    "@discordjs/builders": "~1.6.5",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.0.1",
    "@discordjs/voice": "^0.16.0",
    "@discordx/importer": "^1.2.3",
    "@ppodds/nhentai-api": "^0.0.1",
    "async-lock": "^1.4.0",
    "axios": "^1.5.0",
    "bufferutil": "^4.0.7",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.37.56",
    "discord.js": "^14.13.0",
    "discord.js-message-listener": "^3.0.2",
    "discordx": "^11.7.6",
    "dotenv": "^16.3.1",
    "ehentai-api": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "file-type": "^16.5.4",
    "log4js": "^6.9.1",
    "mariadb": "^3.2.0",
    "node-worker-threads-pool": "^1.5.1",
    "play-dl": "^1.9.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sharp": "~0.32.5",
    "sodium-native": "^4.0.4",
    "tslib": "^2.6.2",
    "tsyringe": "^4.8.0",
    "utf-8-validate": "^6.0.3",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.4",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.14",
    "@types/sequelize": "^4.28.15",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "version": "1.0.0"
}
