{
  "name": "yuets",
  "version": "1.0.0",
  "description": "A TypeScript rewrite version of Discord bot Yue.",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "start": "node --no-deprecation --no-warnings ./dist/index.js",
    "dev": "npm run deploy-cmd && concurrently 'npm run watch' 'nodemon ./dist/index.js'",
    "prod": "npm run build && npm run deploy-cmd  && npm run start",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rm -rf dist/*",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppodds/YueTS.git"
  },
  "author": "ppodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppodds/YueTS/issues"
  },
  "homepage": "https://github.com/ppodds/YueTS#readme",
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@discordjs/voice": "^0.7.5",
    "@psibean/discord.js-pagination": "^4.0.2",
    "async-lock": "^1.3.0",
    "axios": "^0.24.0",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.1",
    "ffmpeg-static": "^4.4.1",
    "file-type": "^17.1.1",
    "log4js": "^6.3.0",
    "mariadb": "^2.5.5",
    "node-worker-threads-pool": "^1.5.1",
    "sequelize": "^6.13.0",
    "sharp": "^0.29.3",
    "sodium": "^3.0.2",
    "utf-8-validate": "^5.0.8",
    "ytdl-core": "^4.10.0",
    "ytdl-core-discord": "^1.3.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.6.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@types/sequelize": "^4.28.11",
    "@types/sharp": "^0.29.5",
    "@types/validator": "^13.7.1",
    "concurrently": "^7.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3"
  }
}
