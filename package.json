{
  "name": "yuets",
  "private": "true",
  "description": "A TypeScript rewrite version of Discord bot Yue.",
  "type": "commonjs",
  "packageManager": "pnpm@8.6.2",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "start": "node --no-deprecation --no-warnings ./dist/index.js",
    "dev": "concurrently 'npm:watch' 'sleep 5;nodemon ./dist/index.js'",
    "test": "jest",
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppodds/YueTS.git"
  },
  "author": "ppodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppodds/YueTS/issues"
  },
  "lint-staged": {
    "*.{ts,js,cjs,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/ppodds/YueTS#readme",
  "dependencies": {
    "@discordjs/builders": "~1.6.1",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.7.0",
    "@discordjs/voice": "^0.16.0",
    "@discordx/importer": "^1.2.2",
    "@ppodds/nhentai-api": "^0.0.1",
    "async-lock": "^1.4.0",
    "axios": "^1.3.5",
    "bufferutil": "^4.0.7",
    "canvas": "^2.11.2",
    "discord-api-types": "^0.37.38",
    "discord.js": "^14.9.0",
    "discord.js-message-listener": "^3.0.2",
    "discordx": "^11.7.4",
    "ehentai-api": "^0.0.4",
    "ffmpeg-static": "^5.1.0",
    "file-type": "^16.5.4",
    "log4js": "^6.9.1",
    "mariadb": "^3.1.1",
    "node-worker-threads-pool": "^1.5.1",
    "play-dl": "^1.9.6",
    "sequelize": "^6.31.0",
    "sharp": "~0.30.7",
    "sodium": "^3.0.2",
    "tslib": "^2.5.0",
    "utf-8-validate": "^6.0.3",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.2",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@types/async-lock": "^1.4.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/sequelize": "^4.28.14",
    "@types/sharp": "^0.30.5",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
