{
  "name": "yuets",
  "private": "true",
  "description": "A TypeScript rewrite version of Discord bot Yue.",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "deploy-cmd": "node ./dist/deploy-commands.js",
    "start": "node --no-deprecation --no-warnings ./dist/index.js",
    "dev": "npm run deploy-cmd && concurrently 'npm:watch' 'sleep 5;nodemon ./dist/index.js'",
    "prod": "npm run build && npm run deploy-cmd && npm run start",
    "test": "jest",
    "clean": "rm -rf dist/*",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ppodds/YueTS.git"
  },
  "author": "ppodds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ppodds/YueTS/issues"
  },
  "lint-staged": {
    "*.{ts,js,cjs,mjs}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "https://github.com/ppodds/YueTS#readme",
  "dependencies": {
    "@discordjs/builders": "^1.0.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "^0.11.0",
    "@ppodds/nhentai-api": "^0.0.1",
    "async-lock": "^1.3.1",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "canvas": "^2.9.1",
    "discord-api-types": "^0.36.3",
    "discord.js": "^14.1.0",
    "discord.js-message-listener": "^2.0.1",
    "ehentai-api": "^0.0.4",
    "ffmpeg-static": "^5.0.0",
    "file-type": "^16.5.4",
    "log4js": "^6.6.0",
    "mariadb": "^3.0.0",
    "node-worker-threads-pool": "^1.5.1",
    "play-dl": "^1.9.5",
    "sequelize": "^6.21.3",
    "sharp": "^0.30.7",
    "sodium": "^3.0.2",
    "utf-8-validate": "^5.0.9",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.6.1",
    "@types/sequelize": "^4.28.11",
    "@types/sharp": "^0.30.2",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
